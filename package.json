{
  "name": "app",
  "version": "0.1.0",
  "main": "server/index.js",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "6.26.0",
    "bcrypt": "^3.0.4",
    "bcrypt-nodejs": "^0.0.3",
    "bootstrap": "^4.3.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cross-env": "^5.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jsonwebtoken": "^8.3.0",
    "luxon": "^1.11.4",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.1",
    "node-sass": "^4.9.3",
    "nodemon": "^1.14.10",
    "object-assign": "4.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "query-string": "^6.3.0",
    "raf": "3.4.0",
    "react": "^16.8.4",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.8.4",
    "react-dropzone": "^6.0.4",
    "react-places-autocomplete": "^7.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.28.6",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  },
  "scripts": {
    "watch-server": "cross-env NODE_ENV=development nodemon app.js --exec babel-node",
    "watch-client": "node scripts/start.js",
    "watch": "concurrently \"yarn watch-client\" \"yarn watch-server\"",
    "build-client": "node scripts/build.js",
    "build-server": "babel -d ./build/config ./config && babel -d ./build/server ./server && babel -d ./build ./app.js",
    "build": "yarn build-client && yarn build-server",
    "dev-build": "cross-env NODE_ENV=development yarn build",
    "prod-build": "cross-env NODE_ENV=production yarn build",
    "build-reset": "rm -rf build",
    "rebuild": "yarn build-reset && yarn build",
    "dev-rebuild": "cross-env NODE_ENV=development yarn rebuild",
    "prod-rebuild": "cross-env NODE_ENV=production yarn rebuild",
    "test": "node scripts/test.js --env=jsdom",
    "start": "node build/app.js",
    "dev-start": "cross-env NODE_ENV=development yarn start",
    "prod-start": "cross-env NODE_ENV=production yarn start",
    "dev": "yarn dev-build && yarn dev-start",
    "prod": "yarn prod-build && yarn prod-start",
    "heroku-postbuild": "yarn build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "server/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/client/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/client/src/**/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/server/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/server/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
